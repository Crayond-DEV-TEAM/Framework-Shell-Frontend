{
  "name": "framworkshell-frontend",
  "version": "1.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:user-management":"turbo run dev --filter=user-management --filter=framwork-shell",
    "build": "turbo run build",
    "dev:production": "turbo run dev:production --parallel",
    "dev:staging": "turbo run dev:staging --parallel",
    "build:development": "turbo run build:development",
    "build:staging": "turbo run build:staging",
    "build:storybook": "turbo run build-storybook",
    "preview": "turbo run preview",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "clean:root": "rm -rf node_modules",
    "clean": "turbo run clean && npm run clean:root ",
    "plop": "plop",
    "prepare": "husky install",
    "commit": "git add . && npx czg",
    "release-boilerplate": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "engines": {
    "node": ">=16.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "packageManager": "npm@8.11.0",
  "devDependencies": {
    "@types/react-json-editor-ajrm": "^2.5.3",
    "eslint-config-custom": "*",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "plop": "^3.1.1",
    "prettier": "latest",
    "semantic-release": "^20.1.1",
    "standard-version": "^9.5.0",
    "turbo": "latest",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@mui/icons-material": "^5.11.11",
    "jsoneditor-react": "^3.1.2",
    "react-json-editor-ajrm": "^2.5.14",
    "react-select": "^5.7.2",
    "react-select-async-paginate": "^0.7.2"
  }
}
