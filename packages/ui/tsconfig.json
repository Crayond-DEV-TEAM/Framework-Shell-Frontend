{
  "extends": "tsconfig/react-library.json",
  "include": ["**/*.ts", "**/*.tsx", "assets/iconSet.tsx", "assets/infoIcon.tsx", "components/dialogContent/.stories.tsx"],
  "exclude": ["dist", "build", "node_modules"],
  "compilerOptions": {
    "types": ["vite/client"],
    "baseUrl": ".",
    "paths": {
      "@atoms/*": [
        "atoms/*"
      ],
      "@atoms": [
        "atoms"
      ],
      "@components/*": [
        "components/*"
      ],
      "@components": [
        "components"
      ],
      "@assets/*": [
        "assets/*"
      ],
      
      "types": ["react", "react-dom", "react-json-editor-ajrm"],
    },
  }
}
